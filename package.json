{
  "name": "obsidian-white-noise",
  "type": "module",
  "version": "0.0.2",
  "description": "Play white noise in Obsidian to help you focus on your work",
  "author": "zhouhua",
  "license": "MIT",
  "keywords": [
    "obsidian",
    "plugin",
    "audio",
    "white noise"
  ],
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
